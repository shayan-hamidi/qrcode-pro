{
  "name": "qrcode-pro",
  "version": "1.0.0",
  "description": "A professional TypeScript QR code generation library with advanced features",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
