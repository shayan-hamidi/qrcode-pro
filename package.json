    "type": "git",
    "url": "git+https://github.com/shayan-hamidi/qrcode-pro.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/qrcode-pro/issues"
  },
  "homepage": "https://github.com/yourusername/qrcode-pro#readme",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.14",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.2",
    "typescript": "^5.9.2"
